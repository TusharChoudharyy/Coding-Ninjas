#include<bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin>>n;
    int sum_e=0;
    int sum_o=0;
    while(n>0){
        int last = n%10;
        if(last%2==0){
            sum_e = sum_e + last;
        }
        else{
            sum_o = sum_o + last;
        }
        n = n/10;
    }
    cout<<sum_e<<" "<<sum_o;
}
